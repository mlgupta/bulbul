<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.0//EN" "http://jakarta.apache.org/commons/dtds/validator_1_0.dtd"><form-validation>
  <global>
  </global>
  <formset>
    <form name="sizeTypeForm">
      <field property="txtSizeTypeId" depends="required,minlength,maxlength">
        <arg0 key="txt.SizeTypeId">
        </arg0>
        <arg1 name="minlength" key="${var:minlength}" resource="false">
        </arg1>
        <arg1 name="maxlength" key="${var:maxlength}" resource="false">
        </arg1>
        <var>
          <var-name>minlength</var-name>
          <var-value>5</var-value>
        </var>
        <var>
          <var-name>maxlength</var-name>
          <var-value>10</var-value>
        </var>
      </field>
      <field property="txaSizeTypeDesc" depends="required,maxlength">
        <arg0 key="txa.SizeTypeDesc">
        </arg0>
        <arg1 name="maxlength" key="${var:maxlength}" resource="false">
        </arg1>
        <var>
          <var-name>maxlength</var-name>
          <var-value>25</var-value>
        </var>
      </field>
    </form>
    <form name="sizeForm">
      <field property="txtSizeId" depends="required,maxlength">
        <arg0 key="txt.SizeId">
        </arg0>
        <arg1 name="maxlength" key="${var:maxlength}" resource="false">
        </arg1>
        <var>
          <var-name>maxlength</var-name>
          <var-value>4</var-value>
        </var>
      </field>
      <field property="txaSizeDesc" depends="required,maxlength">
        <arg0 key="txa.SizeDesc">
        </arg0>
        <arg1 name="maxlength" key="${var:maxlength}" resource="false">
        </arg1>
        <var>
          <var-name>maxlength</var-name>
          <var-value>25</var-value>
        </var>
      </field>
    </form>
  </formset>
</form-validation>
